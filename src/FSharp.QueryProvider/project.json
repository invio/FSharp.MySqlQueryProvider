{
  "version": "1.0.0-*",
  "buildOptions": {
    "debugType": "portable",
    "emitEntryPoint": true,
    "compilerName": "fsc",
    "compile": {
      "includeFiles": [
        "AssemblyInfo.fs",
        "DataReader.fs",
        "Expression.fs",
        "ExpressionMatching.fs",
        "LinqTypes.fs",
        "PreparedQuery.fs",
        "Queryable.fs",
        "QueryOperations.fs",
        "QueryTranslatorUtilities.fs",
        "QueryTranslator.fs"
      ]
    }
  },
  "tools": {
    "dotnet-compile-fsc":"1.0.0-preview2-*"
  },
  "tooling": {
    "defaultNamespace": "FSharp.QueryProvider"
  },
  "dependencies": {
    "MySql.Data": "7.0.5-IR21",
    "Dapper": "1.50.2"
  },
  "frameworks": {
    "netcoreapp1.0": {
      "dependencies": {
        "Microsoft.NETCore.App": {
          "type": "platform",
          "version": "1.0.1"
        },
        "Microsoft.FSharp.Core.netcore": "1.0.0-alpha-160629"
      }
    }
  }
}
