{
  "buildOptions": {
    "debugType": "portable",
    "emitEntryPoint": true,
    "compilerName": "fsc",
    "compile": {
      "includeFiles": [
        "AssertExtensions.fs",
        "LocalDataReader.fs",
        "Models.fs",
        "Utils.fs",
        "DataReaderTest.fs",
        "EmptyQueryProvider.fs",
        "ExpressionTest.fs",
        "MySqlTest.fs"
      ]
    }
  },
  "tools": {
    "dotnet-compile-fsc": {
      "imports": "dnxcore50",
      "version": "1.0.0-preview2-*"
    }
  },
  "dependencies": {
    "FSharp.MySqlQueryProvider": { "target": "project" },
    "Microsoft.NETCore.Platforms": "1.1.0",
    "MySql.Data": "7.0.5-IR21",
    "NETStandard.Library": "1.6.1",
    "dotnet-test-xunit": "2.2.0-*",
    "xunit": "2.2.0-*"
  },

  "runtimes": {
    "win7-x86": { },
    "win7-x64": { },
    "osx.10.11-x64": { },
    "ubuntu.14.04-x64": { },
    "debian.8-x64": { }
  },

  "testRunner": "xunit",

  "frameworks": {
    "netcoreapp1.0": {
      "dependencies": {
        "Microsoft.NETCore.App": "1.1.0",
        "Microsoft.FSharp.Core.netcore": "1.0.0-alpha-160629"
      },
      "imports": [ "netstandard1.1", "netstandard1.6", "portable-net45+win8+wp8+wpa81" ]
    }
  }
}
